    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^7.14.0",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
